generate_adiv_plots <- function(input_data, X, Y, min,max){
data<-as.data.frame(input_data)
#Ensure correct ordering of levels
data$Genotype <- factor(data$Site_General, levels = c("WT", "HET", "MUT"))
data$Day <- factor(data$Day, levels = c("1", "2", "3"))
ggplot(data=data,aes(x={{X}},y={{Y}}, fill={{X}})) +
geom_violin(alpha=0.25,position=position_dodge(width=.75),size=1,color="black",draw_quantiles=c(0.5))+
#geom_boxplot(alpha=0.25)+
geom_quasirandom(alpha=0.1)+
scale_fill_viridis_d()+
geom_point(size=1,position=position_jitter(width=0.25),alpha=0.1)+
theme_cowplot(12) +
ylim(min,max)+
facet_wrap(~Day)+
theme(legend.position = "none")
}
